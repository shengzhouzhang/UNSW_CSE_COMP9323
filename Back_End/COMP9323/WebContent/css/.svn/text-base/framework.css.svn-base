div.modal
{
	display: none;
	position: fixed;
	opacity:0.1;
	background-color: #888;
    left: 0px;
    top: 0px;
    width:100%;
    height:100%;
    text-align:center;
    z-index: 700;
}

div.messagebox
{
	position: fixed;
	min-width: 200px;
	top: 0px;
	margin: 10px auto 0 49%;
	padding: 10px;
	z-index: 600;
	background-color: green;
	font-weight: bold;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	color: white;
	text-align: center;
}

div.alert
{
	background-color: red;
}

#fileList a.cancel
